<%= form_for(@pin) do |f| %>

	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :resource_type %>
	<%= f.text_field :resource_type %>

	<%= f.label :text %>
	<%= f.text_area :text %>

	<%= f.label :url %>
	<%= f.text_field :url %>

	<%= f.label :slug %>
	<%= f.text_field :slug %>

	<%= f.submit %>

<% end %>